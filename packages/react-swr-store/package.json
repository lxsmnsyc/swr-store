{
  "version": "0.10.2",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/production/index.js",
  "module": "dist/esm/production/index.js",
  "exports": {
    ".": {
      "development": {
        "require": "./dist/cjs/development/index.js",
        "import": "./dist/esm/development/index.js"
      },
      "require": "./dist/cjs/production/index.js",
      "import": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dev": {
      "production": {
        "require": "./dist/cjs/production/index.js",
        "import": "./dist/esm/production/index.js"
      },
      "require": "./dist/cjs/development/index.js",
      "import": "./dist/esm/development/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./esm": {
      "development": "./dist/esm/development/index.js",
      "production": "./dist/esm/production/index.js",
      "default": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./cjs": {
      "development": "./dist/cjs/development/index.js",
      "production": "./dist/cjs/production/index.js",
      "default": "./dist/cjs/production/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "name": "react-swr-store",
  "devDependencies": {
    "@types/node": "^17.0.9",
    "@types/react": "^17.0.19",
    "eslint": "^8.7.0",
    "eslint-config-lxsmnsyc": "^0.4.0",
    "pridepack": "^1.0.4",
    "react": "^17.0.2",
    "swr-store": "0.10.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0",
    "swr-store": "^0.9"
  },
  "scripts": {
    "prepublish": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "lint": "pridepack lint",
    "test": "pridepack test --passWithNoTests",
    "clean": "pridepack clean",
    "watch": "pridepack watch"
  },
  "description": "React bindings for swr-store",
  "author": "Alexis Munsayac",
  "private": false,
  "repository": {
    "url": "https://github.com/lxsmnsyc/swr-store.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/swr-store/tree/main/packages/react-swr-store",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/swr-store/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@lyonph/react-hooks": "^0.5.1",
    "react-store-adapter": "^0.4.4"
  }
}
